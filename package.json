{
  "name": "kevast-gist",
  "version": "0.0.4",
  "description": "Gist storage for kevast.js",
  "main": "dist/nodejs/index.js",
  "keywords": [
    "kevast",
    "key",
    "value",
    "key-value",
    "storage",
    "redundancy",
    "gist"
  ],
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "mocha --no-timeouts --require espower-typescript/guess \"test/**/*.ts\"",
    "build": "yarn build:nodejs && yarn build:browser",
    "build:browser": "webpack",
    "build:nodejs": "tsc",
    "clean": "rm -rf ./dist/*",
    "prepublishOnly": "yarn test && yarn build",
    "coverage": "istanbul cover -e .ts _mocha -- --no-timeouts --require ts-node/register \"test/**/*.ts\"",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": "git@github.com:kevast/kevast-gist.js.git",
  "author": "Andie <zchangan@163.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.2",
    "espower-typescript": "^9.0.1",
    "istanbul": "1.1.0-alpha.1",
    "kevast": "^0.2.4",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
