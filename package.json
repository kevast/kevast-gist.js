{
  "name": "kevast-gist",
  "version": "0.0.2",
  "description": "Gist storage for kevast.js",
  "main": "dist/nodejs/index.js",
  "keywords": [
    "kevast",
    "key",
    "value",
    "key-value",
    "storage",
    "redundancy",
    "gist"
  ],
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "mocha -t 20000 --require espower-typescript/guess \"test/**/*.ts\"",
    "build": "yarn build:nodejs && yarn build:browser",
    "build:browser": "webpack",
    "build:nodejs": "tsc",
    "clean": "rm -rf ./dist/*",
    "prepublishOnly": "yarn test && yarn build"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": "git@github.com:kevast/kevast-gist.js.git",
  "author": "Andie <zchangan@163.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/axios": "^0.14.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.0.5",
    "espower-typescript": "^9.0.1",
    "kevast": "^0.1.8",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
